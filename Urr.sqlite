DROP TABLE IF EXISTS "Notificacao";
CREATE TABLE "Notificacao" ("id" INTEGER PRIMARY KEY  AUTOINCREMENT  NOT NULL  UNIQUE , "data" DATETIME, "tipo" VARCHAR, "historico" VARCHAR, paciente INTEGER REFERENCES Paciente(id), plantonista INTEGER REFERENCES Plantonista(id), upaCrd INTEGER REFERENCES UpaCrs(id));
DROP TABLE IF EXISTS "Paciente";
CREATE TABLE "Paciente" ("id" INTEGER PRIMARY KEY  AUTOINCREMENT  NOT NULL  UNIQUE , "nome" VARCHAR, "cpf" VARCHAR, "rg" VARCHAR, "endereco" VARCHAR, "dataNascimento" DATETIME, "telefone" VARCHAR, "numeroProntuario" VARCHAR);
DROP TABLE IF EXISTS "Plantonista";
CREATE TABLE "Plantonista" ("id" INTEGER PRIMARY KEY  AUTOINCREMENT  NOT NULL  UNIQUE , "nome" VARCHAR, "telefone" VARCHAR, "lotacao" VARCHAR);
DROP TABLE IF EXISTS "UpaCrs";
CREATE TABLE "UpaCrs" ("id" INTEGER PRIMARY KEY  AUTOINCREMENT  NOT NULL  UNIQUE , "telefone" VARCHAR, "endereco" VARCHAR);
